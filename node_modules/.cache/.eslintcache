 e.preventDefault();\n    let trip = {\n      destination: this.state.destination,\n      landscape: this.state.landscape,\n      tripDate: this.state.tripDate,\n      tripLength: this.state.tripLength,\n      tripBudget: this.state.tripBudget,\n      groupSize: this.state.groupSize,\n      expectedAgeGroup: this.state.expectedAgeGroup,\n      expectedGender: this.state.expectedGender,\n      description: this.state.description,\n    };\n    console.log(trip);\n    TripServices.saveTrip(trip).then((res) => {\n      console.log(res.data.tripId);\n      var newtripId = res.data.tripId;\n      localStorage.setItem(\"tripId\", newtripId);\n      this.props.history.push(\"/newMatch\");\n    });\n  };\n\n  render() {\n    const {\n      destination,\n      landscape,\n      tripDate,\n      tripLength,\n      tripBudget,\n      groupSize,\n      expectedAgeGroup,\n      expectedGender,\n      description,\n    } = this.state;\n    return (\n      <div>\n        <Form>\n          <Row className=\"justify-content-md-center\">\n            <Col xs={7}>\n              <Card className={\"border border-dark bg-dark text-white\"}>\n                <Card.Header className=\"mb-2 mt-2\">\n                  <h2>\n                    <FontAwesomeIcon icon={faUserPlus} /> Trip Preferences\n                  </h2>\n                </Card.Header>\n                <Card.Body>\n                  <Form.Group controlId=\"formGridDestination\">\n                    <Form.Label>Destination</Form.Label>\n                    <Form.Control\n                      type=\"text\"\n                      placeholder=\"Enter Destination\"\n                      name=\"destination\"\n                      value={destination}\n                      onChange={this.userChange}\n                    />\n                  </Form.Group>\n\n                  <Form.Group controlId=\"formGridLandscape\">\n                    <Form.Label>Landscape</Form.Label>\n                    <Form.Control\n                      as=\"select\"\n                      defaultValue=\"Choose Landsape...\"\n                      name=\"landscape\"\n                      //   value={landscape}\n                      onChange={this.userChange}\n                    >\n                      <option value=\"-1\">Choose Landscape...</option>\n                      <option value=\"0\">Any</option>\n                      <option value=\"1\">Mountain/Hilly</option>\n                      <option value=\"2\">Beach</option>\n                      <option value=\"3\">Snowy</option>\n                      <option value=\"4\">Other</option>\n                    </Form.Control>\n                  </Form.Group>\n\n                  <Form.Group controlId=\"formGridDate\">\n                    <Form.Label>Travel Date</Form.Label>\n                    <Form.Control\n                      type=\"date\"\n                      name=\"tripDate\"\n                      value={tripDate}\n                      onChange={this.userChange}\n                    />\n                  </Form.Group>\n\n                  <Form.Group controlId=\"formGridTripLength\">\n                    <Form.Label>Trip Length</Form.Label>\n                    <Form.Control\n                      placeholder=\"Enter no. of days\"\n                      name=\"tripLength\"\n                      value={tripLength}\n                      onChange={this.userChange}\n                    />\n                  </Form.Group>\n\n                  <Form.Row>\n                    <Form.Group as={Col} controlId=\"formGridBudget\">\n                      <Form.Label>Trip Budget</Form.Label>\n                      <Form.Control\n                        placeholder=\"Enter your budget\"\n                        name=\"tripBudget\"\n                        value={tripBudget}\n                        onChange={this.userChange}\n                      />\n                    </Form.Group>\n\n                    <Form.Group as={Col} controlId=\"formGridGroup\">\n                      <Form.Label>Group Size</Form.Label>\n                      <Form.Control\n                        placeholder=\"Your group size\"\n                        name=\"groupSize\"\n                        value={groupSize}\n                        onChange={this.userChange}\n                      />\n                    </Form.Group>\n                  </Form.Row>\n\n                  <Form.Row>\n                    <Col xs={6}>\n                      <Form.Group controlId=\"formGridAge\">\n                        <Form.Label>Preferred Age Group</Form.Label>\n                        <Form.Control\n                          as=\"select\"\n                          defaultValue=\"Choose...\"\n                          name=\"expectedAgeGroup\"\n                          //   value={expectedAgeGroup}\n                          onChange={this.userChange}\n                        >\n                          <option value=\"-1\">Choose...</option>\n                          <option value=\"0\">Upto 20 years</option>\n                          <option value=\"1\">20-30</option>\n                          <option value=\"3\">30-40</option>\n                          <option value=\"4\">40-50</option>\n                          <option value=\"5\">50+</option>\n                        </Form.Control>\n                      </Form.Group>\n                    </Col>\n\n                    <Form.Group as={Col}>\n                      <Form.Label>Preferred Gender</Form.Label>\n                      <Col sm={10}>\n                        <Form.Check\n                          type=\"radio\"\n                          label=\"Any\"\n                          name=\"expectedGender\"\n                          id=\"anyradio\"\n                          value=\"0\"\n                          onChange={this.userChange}\n                        />\n                        <Form.Check\n                          type=\"radio\"\n                          label=\"Male\"\n                          name=\"expectedGender\"\n                          id=\"maleradio\"\n                          value=\"1\"\n                          onChange={this.userChange}\n                        />\n                        <Form.Check\n                          type=\"radio\"\n                          label=\"Female\"\n                          name=\"expectedGender\"\n                          id=\"femaleradio\"\n                          value=\"2\"\n                          onChange={this.userChange}\n                        />\n                      </Col>\n                    </Form.Group>\n                  </Form.Row>\n\n                  <Form.Group controlId=\"formGridDescription\">\n                    <Form.Label>Description</Form.Label>\n                    <Form.Control\n                      type=\"textarea\"\n                      rows=\"9\"\n                      placeholder=\"Your expectations here...\"\n                      name=\"description\"\n                      value={description}\n                      onChange={this.userChange}\n                    />\n                  </Form.Group>\n                </Card.Body>\n                <Card.Footer style={{ textAlign: \"left\" }}>\n                  <Button\n                    variant=\"primary\"\n                    type=\"submit\"\n                    onClick={this.submitTrip}\n                  >\n                    Find Me Some Fellow Travellers\n                  </Button>\n                </Card.Footer>\n              </Card>\n            </Col>\n          </Row>\n        </Form>\n      </div>\n    );\n  }\n}\n\nexport default CreateTrip;\n","/home/rishabh/IIITB/SEMII/SPE/Project/project-frontend/tripshare/src/components/ChatUI.js",["134","135","136"],"import { Button, Card } from \"react-bootstrap\";\nimport React, { Component } from \"react\";\nimport TextField from \"@material-ui/core/TextField\";\nimport ChatServices from \"../services/ChatServices\";\nimport \"..