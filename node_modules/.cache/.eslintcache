ch from \"./components/NewMatch\";\nimport DeleteProfile from \"./components/DeleteProfile\";\nimport ChangePhoto from \"./components/ChangePhoto\";\nimport ChatUI from \"./components/ChatUI\";\n\nimport MyTrips from \"./components/MyTrips\";\nimport AboutUs from \"./components/AboutUs\";\nimport \"bootstrap/dist/css/bootstrap.min.css\";\n\nexport default function App() {\n  return (\n    <Router>\n      <NavigationBar />\n      <Container>\n        <Row>\n          <Col lg={12} className={\"margin-top\"}>\n            <Switch>\n              <Route path=\"/\" exact component={Welcome} />\n              <Route path=\"/register\" exact component={Register} />\n              <Route path=\"/login\" exact component={Login} />\n              <Route path=\"/dashboard\" exact component={Dashboard} />\n              <Route path=\"/editProfile\" exact component={EditProfile} />\n              <Route path=\"/createtrip\" exact component={CreateTrip} />\n              <Route path=\"/newMatch\" exact component={NewMatch} />\n              <Route path=\"/deleteProfile\" exact component={DeleteProfile} />\n              <Route path=\"/changePhoto\" exact component={ChangePhoto} />\n              <Route path=\"/chatUi/:id\" exact component={ChatUI} />\n              <Route path=\"/myTrips\" exact component={MyTrips} />\n              <Route path=\"/aboutUs\" exact component={AboutUs} />\n            </Switch>\n          </Col>\n        </Row>\n      </Container>\n    </Router>\n  );\n}\n","/home/rishabh/IIITB/SEMII/SPE/Project/project-frontend/tripshare/src/serviceWorker.js",[],"/home/rishabh/IIITB/SEMII/SPE/Project/project-frontend/tripshare/src/services/auth/store.js",[],"/home/rishabh/IIITB/SEMII/SPE/Project/project-frontend/tripshare/src/components/NavigationBar.js",[],"/home/rishabh/IIITB/SEMII/SPE/Project/project-frontend/tripshare/src/components/Welcome.js",[],"/home/rishabh/IIITB/SEMII/SPE/Project/project-frontend/tripshare/src/components/Register.js",[],"/home/rishabh/III